  <script>
      $("#login_mgt").click(function(){
          var username=$("#username_mgt").val();
          var password=$("#password_mgt").val();
          var data={"uname":username,"upwd":password};
          $.ajax({
              url:'/login',
              type:'post',
              data:data,
              success:function(data,status){
                  var obj = JSON.parse(data);
                  if(obj.type=="1"){
                      //alert(obj.type);
                      location.href = 'index';
                  }else{
                      //alert(obj.type);
                      location.href = 'login';
                  }
              },
              error:function(data,status){
              }
          })
      });
      console.log(encodeURIComponent("http://www.timemgt.com.cn/qdzn/view/index.html"))
